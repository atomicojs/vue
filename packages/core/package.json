{
    "name": "@atomico/vue",
    "version": "0.2.0",
    "description": "Wrapper and utilities to integrate Atomico in Vue",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**",
        "build:exports": "exports lib/** types/**",
        "prepublishOnly": "npm run exports"
    },
    "keywords": [],
    "devDependencies": {
        "@atomico/tsconfig": "^1.0.0"
    },
    "dependencies": {
        "@atomico/wrapper": "^1.3.0",
        "atomico": "latest",
        "vue": "*"
    },
    "peerDependencies": {
        "atomico": "latest",
        "vue": "*"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    },
    "author": "UpperCod",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/atomicojs/vue/issues"
    },
    "homepage": "https://github.com/atomicojs/vue#readme",
    "exports": {
        ".": "./lib/index.js",
        "./index.d.ts": "./types/index.d.ts",
        "./wrapper": "./lib/wrapper.js",
        "./wrapper.d.ts": "./types/wrapper.d.ts"
    },
    "typesVersions": {
        "*": {
            "wrapper": [
                "types/wrapper.d.ts"
            ]
        }
    },
    "types": "./types/index.d.ts"
}
